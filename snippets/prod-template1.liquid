          <div class="return-policy_icon">{{section.settings.return_policy_icon}}</div>
          <div class="return-policy_title">{{section.settings.return_policy_heading}}</div>
        </div>
        <div class="return-policy_body">{{section.settings.return_policy_content}}</div>
      </div>
