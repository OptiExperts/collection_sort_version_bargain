        <label class="txt">
          <small> <i class="fa fa-bar-chart"> </i>&nbsp;<a class="size-chart-open-popup"
                                                           href="#size-chart">{{ 'products.product.size_chart' | t }}</a> </small>
        </label>
      </div>
      {% endif %}
      {% endif %}
