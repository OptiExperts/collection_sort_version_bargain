      {% include 'countdown' %}
      {% endif %}

      {% capture "form_classes" -%}
      product-form
      product-form-{{ section.id }}{% if section.settings.enable_payment_button and product.has_only_default_variant%}
      product-form--payment-button-no-variants{% endif %}
