<style>.promo-countainer {text-align: center;line-height: 7pt;width: 100%;}.promo-countainer img {max-width: 50px;height: 50px;overflow: hidden;width: 100%;}.promo-countainer .h3 {display: flex;width: 100%;justify-content: center;align-items: center;text-align: center;color: {{settings.ctdn_hdclr}};}.promo-countainer .h4 {color: {{settings.ctdn_subhdclr}};}.promo-countainer p {color: {{settings.coupon_txt}};}.blur-img {filter: blur(2px);-webkit-filter: blur(2px);}.coupon-section input{text-align:center;margin:5px;padding:5px;}.tooltip-coupon{position: relative;display: inline-block;padding-bottom : 25px;}.tooltip-coupon .tooltipcoupon{visibility: hidden;width: auto;background-color:{{settings.coupon-bk-tooltip-clr}};color:{{settings.coupon-txt-tooltip-clr}};text-align: center;border-radius: 6px;padding: 5px;position: absolute;z-index: 1;bottom: 150%;left: 50%;margin-left: -75px;opacity: 0;transition: opacity 0.3s;}.tooltip-coupon .tooltipcoupon::after{content: "";position: absolute;top: 100%;left: 50%;margin-left: -5px;border-width: 5px;border-style: solid;border-color: #555 transparent transparent transparent;}.tooltip-coupon:hover .tooltipcoupon{visibility:visible;opacity:1;}.call{display:inline-block;text-decoration:none;border-radius:5px;padding:10px;background:{{settings.coupon-bk-clr}};max-width:40%;font:bold 24px Arial;box-shadow:0 0 15px gray,0 0 10px gray inset;text-align:center;color:{{settings.coupon-txt-clr}} !important;}.tooltip-coupon button{background:#007367;color:#FFFFFF;border:none;}</style>  
