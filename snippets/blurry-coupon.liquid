{% assign filter-code-show = collections[settings.filter-code] %}
{% assign foundProduct = false %}
{% assign currentProductTitle = product.title %}
{% if filter-code-show.products.size > 0 %}
{% for product in filter-code-show.products %}
	{% if product.title == currentProductTitle %}
		{% assign foundProduct = true %}
	{% endif %}
{% endfor %}
{% endif %}
{% if foundProduct == true %}
<div id="talltweets" style="display: inline-block;visibility: hidden;" >
{{settings.promo_code}}
</div>
<div class="promo-countainer">    
  <div class="h3"> {{settings.promo_hd}} </div>
  <div class="h4"> {{settings.promo_subhd}} </div>
  {% unless customer %}
 <img class="blur-img" id="textScreenshot" src="" alt="Coupon code">
  
  {% endunless %}
{% if customer %} 
   <input class="h4 call" value="{{settings.promo_code}}" id="myInput" readonly>
   <div class="tooltip-coupon">
      <button onclick="myFunction()" onmouseout="outFunc()">
      <span class="tooltipcoupon" id="myTooltip">{{settings.coupon-tooltip}}</span>
      {{settings.coupon-txt}}
      </button>
   </div>  
  {% else %}
<br><p> {{settings.promo_before}} <a href="/account/register?url={{ theme_role | theme_url }}" title="{{ 'customer.register.title' | t }}">{{ 'customer.register.title' | t }}</a>/<a href="/account/login?url={{ theme_role | theme_url }}" title="{{ 'customer.login.sign_in' | t }}">{{ 'customer.login.sign_in' | t }}</a> {{settings.promo_after}} <p>
{% endif %}
