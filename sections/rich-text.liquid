<style>.rich-text--{{section.id}} h1, .rich-text--{{section.id}} h2, .rich-text--{{section.id}} h3 {color : {{section.settings.rich-txt-hd-clr}};font-size : {{section.settings.richtext_hd_sz}}px;  }.rich-text--{{section.id}} p {  color : {{section.settings.rich-txt-clr}};font-size : {{section.settings.richtext_txt_sz}}px;    }</style>  
<div class="page-width rich-text--{{section.id}} {{section.settings.richtext-visibility}}">
    <div class="grid__item {% unless section.settings.full_width %} medium-up--two-thirds medium-up--push-one-sixth{% endunless %} text-{{section.settings.rich-txt-align}}"
         style="background-color: {{section.settings.rich-txt-bk}};opacity:{{section.settings.rich-txt-bk-op}}%">
      {% if section.settings.section_title != blank %}
      {% if section.settings.richtext-hd == 'h1' %}
          <h1 class="h3">{{ section.settings.section_title | escape }}</h1>
      {% elsif section.settings.richtext-hd == 'h2' %}
          <h2 class="h3">{{ section.settings.section_title | escape }}</h2>   
      {% else %}
          <h3 class="h3">{{ section.settings.section_title | escape }}</h3>   
      {% endif %}
      {% endif %}
      {% if section.settings.text != blank %}
        <div>{{ section.settings.text }}</div>
      {% endif %}
    </div>
</div>
