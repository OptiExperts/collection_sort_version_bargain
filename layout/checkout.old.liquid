      @media (min-width: 1000px) {
        body.test_75 .trustpilot-custom--mobile {display: none !important;}
        body.test_75 .trustpilot-custom--desktop {display: flex !important;align-items: center;flex-wrap: wrap;margin-bottom: 10px;}
        .tp-stars {flex: 1;display: flex;}
        .tp-text {width: 100%;margin-top: 12px;}
        .tp-text--desktop {position: relative;display: inline-block;margin-bottom: 12px;}
        .trustpilot-custom--desktop .tp-stars >div {padding-bottom: 20px !important;flex: 0 0 125px;margin-right: 30px;}
        .trustpilot-custom--desktop .tp-stars >div svg {height: 20px !important;width: auto !important;}
        .tp-text {font-size: 16px;}
      }
      /* 1712022 */
      .checkout-confidence-blocks { display: flex;
        background: #5B8BA9;
        color: #fff;
        font-size: 10px;
        justify-content: center;
        align-items: center;
        margin-top: 20px;
      }
      .confidence-item {
      	padding: 3.5px 5px 4.5px;
        display: flex;
        align-items: center;
        font-family: 'Montserrat', sans-serif;
      }
      .confidence-item svg {
      	width: 12px;
        height: 12px;
        margin-right: 5px;
      }
      .test_84 .banner {
      	padding-bottom: 0;
      }
      @media (min-width: 1000px) {
        .logo-vs-confidence {
          display: flex;
          margin-bottom: 24px;
        }
        .logo-vs-confidence .show-test_84 {
        	flex: 1;
        }
        .logo-vs-confidence .show-test_84 .logo {
          height: 100%;
          margin-left: 35px;
        }
        .logo-vs-confidence .checkout-confidence-blocks {
          height: 100%;
          margin-top: 0;
          border-radius: 5px;
          max-width: 400px;
          margin-left: auto;
        }
        .logo-vs-confidence .confidence-item {
          flex-wrap: wrap;
          text-align: center;
          justify-content: center;
        }
        .logo-vs-confidence .confidence-item span {
        	width: 100%;
        }
        .logo-vs-confidence .confidence-item svg {
          margin: 0 0 5px;
          width: 15px;
          height: 15px;
        }
      }
    </style>


